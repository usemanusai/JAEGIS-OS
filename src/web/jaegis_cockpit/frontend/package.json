{
  "name": "jaegis-cockpit-frontend",
  "version": "1.0.0",
  "description": "JAEGIS Cockpit - Frontend Dashboard",
  "private": true,
  "scripts": {
    "build": "vite build",
    "dev": "vite dev",
    "preview": "vite preview",
